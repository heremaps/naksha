            <stringProp name="parameters"></stringProp>
            <stringProp name="script">// Read the response, extract one random feature, modify that feature
// Extract also feature id
import groovy.json.JsonSlurper
import groovy.json.JsonOutput
import java.util.Random

String response = prev.getResponseDataAsString()

def jsonResponse = new JsonSlurper().parseText(response)

// Keep only one random feature for the next test
int randomIndex = new Random().nextInt(jsonResponse.features.size())
jsonResponse.features = [jsonResponse.features[randomIndex]]

jsonResponse.features[0].properties.&quot;@ns:com:here:xyz&quot;.tags=[&quot;${g_execute_time}&quot;]

String modifiedFeature = JsonOutput.toJson(jsonResponse)
// Use the modified feature and ID for the next tests
vars.put(&quot;v_modified_feature_collection&quot;, modifiedFeature)
vars.put(&quot;v_modified_feature_id&quot;, jsonResponse.features[0].id)</stringProp>
